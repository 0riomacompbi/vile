    "@brentlintner/vile-reek": "0.0.1",
    "@brentlintner/vile-rubocop": "0.0.4",
    "@brentlintner/vile-rubycritic": "0.0.2",
    "@brentlintner/vile-semver": "0.0.1",
    "@brentlintner/vile-tslint": "0.0.2",
    "@brentlintner/vile-whitespace": "0.0.2",
    "babel": "~5.8.29",
    "chai": "~3.4.0",
    "coffee-script": "~1.10.0",
