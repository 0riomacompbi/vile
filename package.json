{
  "name": "@brentlintner/vile",
  "version": "0.3.6",
  "description": "A punishing, yet easy to use tool for writing insightful code.",
  "main": "lib/index.js",
  "files": [
    "bin/vile",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "vile": "bin/vile"
  },
  "scripts": {
    "compile": "tsc --outDir .build --target es6 src/*.ts && babel -d lib -m common .build",
    "compile-dev": "coffee -cb test",
    "dev": "bin/dev",
    "dev-setup": "bin/dev-setup",
    "lint": "bin/lint",
    "lint-cov": "bin/lint-cov",
    "update": "police -lt package.json && constable -u && tsd update -so",
    "tsd": "tsd reinstall -so",
    "test": "bin/test",
    "test-cov": "bin/test-cov",
    "test-ci-build": "npm run punish && bin/test-ci-build"
  },
  "author": "Brent Lintner <brent.lintner@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/brentlintner/vile/issues"
  },
  "homepage": "https://github.com/brentlintner/vile",
  "repository": {
    "type": "git",
    "url": "https://github.com/brentlintner/vile.git"
  },
  "keywords": [
    "quality",
    "analyze",
    "analyse",
    "lint",
    "jshint",
    "eslint",
    "coffeelint",
    "tslint",
    "hlint",
    "git",
    "language",
    "coverage",
    "check",
    "code",
    "tool"
  ],
  "dependencies": {
    "bluebird": "~2.10.2",
    "cli-spinner": "~0.2.1",
    "commander": "~2.9.0",
    "extend": "~3.0.0",
    "ignore-file": "~1.1.2",
    "js-yaml": "~3.4.3",
    "lodash": "~3.10.1",
    "minilog": "~2.0.8",
    "request": "~2.64.0",
    "string-padder": "~1.0.3"
  },
  "devDependencies": {
    "@brentlintner/vile-coffeelint": "0.0.3",
    "@brentlintner/vile-constable": "0.0.2",
    "@brentlintner/vile-depcheck": "0.0.1",
    "@brentlintner/vile-eclint": "0.0.1",
    "@brentlintner/vile-eslint": "0.0.2",
    "@brentlintner/vile-fs-lint": "0.0.1",
    "@brentlintner/vile-git": "0.0.1",
    "@brentlintner/vile-htmllint": "~0.1.1",
    "@brentlintner/vile-jshint": "0.0.1",
    "@brentlintner/vile-language": "0.0.2",
